{
  "name": "tictrack",
  "version": "1.0.0",
  "description": "Tictrack RESTful APIs using Hono",
  "main": "dist/index.mjs",
  "engines": {
    "node": ">=12.0.0"
  },
  "bin": "bin/createApp.js",
  "keywords": [
    "cloudflare",
    "workers",
    "cloudflare-worker",
    "cloudflare-workers",
    "planetscale",
    "boilerplate",
    "template",
    "starter",
    "example",
    "jest",
    "hono",
    "api",
    "rest",
    "sql",
    "oauth",
    "jwt",
    "es6",
    "es7",
    "es8",
    "es9",
    "jwt",
    "zod",
    "eslint",
    "prettier"
  ],
  "scripts": {
    "start": "bun run --hot app/src/index.ts",
    "dev": "bun run --hot app/src/index.ts",
    "run": "bun run app/src/index.ts",
    "lint": "eslint . --ext .ts",
    "lint:files": "eslint --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "prettier": "prettier --check **/*.ts",
    "prettier:fix": "prettier --write **/**/*.ts",
    "prepare": "husky install",
    "test": "jest",
    "lint:biome": "biome lint ./app/src",
    "format:biome": "biome format ./app/src"
  },
  "dependencies": {
    "@cloudflare/workers-types": "^4.20240529.0",
    "@faker-js/faker": "^8.4.1",
    "@hono/eslint-config": "^0.0.6",
    "@hono/node-server": "^1.11.1",
    "@hono/sentry": "^1.1.0",
    "@hono/swagger-ui": "^0.2.2",
    "@hono/zod-openapi": "^0.14.0",
    "@hono/zod-validator": "^0.2.1",
    "@prisma/client": "5.7.1",
    "@tsndr/cloudflare-worker-jwt": "^2.5.3",
    "@types/bcryptjs": "^2.4.6",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.6",
    "@typescript-eslint/eslint-plugin": "^7.11.0",
    "@typescript-eslint/parser": "^7.11.0",
    "@upstash/redis": "^1.31.3",
    "add": "^2.0.6",
    "aws-sdk-client-mock": "^4.0.0",
    "aws-sdk-client-mock-jest": "^4.0.0",
    "bcryptjs": "^2.4.3",
    "dayjs": "^1.11.11",
    "dotenv": "^16.4.5",
    "esbuild": "^0.21.4",
    "eslint": "^9.3.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^28.5.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "hono": "^4.4.0",
    "http-status": "^1.7.4",
    "ioredis": "^5.4.1",
    "jest": "^29.7.0",
    "jest-environment-miniflare": "^2.14.2",
    "jsonwebtoken": "^9.0.2",
    "kysely": "^0.27.3",
    "kysely-planetscale": "^1.4.0",
    "miniflare": "^3.20240524.0",
    "mockdate": "^3.0.5",
    "otplib": "^12.0.1",
    "prettier": "^3.3.0",
    "redis": "^4.6.14",
    "toucan-js": "^3.4.0",
    "ts-jest": "^29.1.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5",
    "undici": "^6.18.2",
    "wrangler": "^3.60.0",
    "zod": "^3.23.8",
    "zod-error": "^1.5.0"
  },
  "devDependencies": {
    "@biomejs/biome": "1.8.0",
    "@types/bun": "latest",
    "husky": "^9.0.11",
    "prisma": "5.7.1"
  }
}
